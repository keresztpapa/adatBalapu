<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <link rel="stylesheet" href="stylesheets/style.css">
  <title>Könyvesbolt</title>
</head>
<body>
    <!-- Navbar -->
    <% include navbar %>
    
    <!-- Content area -->
    <div class="content">
      <h2> Könyvek </h2>

      <!-- Konyvek listazasa -->
      <h1>Query Results</h1>
      <table>
        <thead>
          <tr>
            <th>Column 1</th>
            <th>Column 2</th>
            <th>Column 3</th>
            <th>Column 4</th>
            <!-- Add more column headers here -->
          </tr>
        </thead>
        <tbody>
          <% rows.forEach(function(row) { %>
          <tr>
            <td><%= row[0] %></td>
            <td><%= row[1] %></td>
            <td><%= row[2] %></td>
            <td><%= row[3] %></td>
            <!-- Add more columns here -->
          </tr>
          <% }); %>
        </tbody>
      </table>
    </div>
  </body>
</html>